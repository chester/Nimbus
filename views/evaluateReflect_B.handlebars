<body>

    <div id="title">
        <h3>Pause and evaluate your motivations</h3>
        <h3 id="focus"></h3>
    </div>


   

        <div class="questions">
            <input type="text" id="q1" placeholder="Why did you select this to focus on?">
            <input type="text" id="q2" placeholder="What do you want out of this?">
            <input type="text" id="q3" placeholder="How do you feel towards this?">
            <input type="text" id="q4" placeholder="Lastly, write an affirmation for yourself.">
        </div>

    <div class="nextQDiv">
        <button id="next">Next Question</button>
        <button id="next2">Next Question</button>
        <button id="last">Last Question</button>
    </div>

    <div class="backButton">
        <a href="/selectReflect">
            <button id="back">Back</button>
        </a>
    </div>

    <div class="saveButton">
        <a href="/stepsReflect">
            <button id="save" onclick="nextPage()">Save</button>
        </a>
    </div>

    <div class="statusDiv">
        <img id="status" src="https://i.imgur.com/p20fDtG.png">
    </div>

    <script src="js/jquery-1.11.0.js"></script>
    <script src="js/bootstrap.js"></script>
    <script src="js/evaluateReflect_B.js"></script>

</body>



</html>